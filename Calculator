#include <stdio.h>

int main()
{
	double num1, num2;
	char operation;
	while (1)
	{
		scanf("%lf%c%lf", &num1, &operation, &num2);
		if (operation == '+')
			printf("%0.2lf\n", num1 + num2);
		if (operation == '-')
			printf("%0.2lf\n", num1 - num2);
		if (operation == '*')
			printf("%0.2lf\n", num1 * num2);
		if (operation == '/')
			printf("%0.2lf\n", num1 / num2);
	}
	return 0;
}